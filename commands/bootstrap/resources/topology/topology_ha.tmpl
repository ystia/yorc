tosca_definitions_version: alien_dsl_2_0_0

metadata:
  template_name: YorcHa
  template_version: 0.1.0-SNAPSHOT
  template_author: Yorc

description: ""

imports:
  - alien-base-types/{{getAlien4CloudVersionFromTOSCATypes}}/alien-base-types.yml
  - org.ystia.nfs.linux.ansible/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - <normative-types.yml>
  - org.alien4cloud.alien4cloud.config.location/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - <yorc-google-types.yml>
  - <yorc-openstack-types.yml>
  - <yorc-aws-types.yml>
  - <yorc-hostspool-types.yml>
  - org.ystia.yorc.experimental.consul.linux.ansible/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.alien4cloud.elasticsearch.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.ystia.terraform.linux.terraform/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.alien4cloud.java.jmx.jolokia/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.consul.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.ystia.yorc.linux.ansible/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.ystia.nfs.pub/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - <yorc-types.yml>
  - org.alien4cloud.java.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.ystia.yorc.pub/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.alien4cloud.alien4cloud.config.location_resources.on_demand/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.alien4cloud.config.location_resources.autoconfig/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.ystia.yorc.experimental.consul.pub/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.ystia.ansible.linux.ansible/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.alien4cloud.alien4cloud.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.alien4cloud.webapp/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.ystia.yorc.alien4cloud/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.alien4cloud.java.jdk.linux/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.alien4cloud.config.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.cloudify.hostpool.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/types.yml
  - org.alien4cloud.vault.pub/{{getAlien4CloudForgeVersionFromTOSCATypes}}/type.yml
  - org.alien4cloud.vault.vault_sh/{{getAlien4CloudForgeVersionFromTOSCATypes}}/type.yml
  - alien-extended-storage-types/{{getAlien4CloudVersionFromTOSCATypes}}/alien-extended-storage-types.yml
  - org.ystia.terraform.pub/{{getForgeVersionFromTOSCATypes}}/types.yaml
  - org.ystia.ansible.pub/{{getForgeVersionFromTOSCATypes}}/types.yaml

topology_template:
  node_templates:
    ConsulServer:
      type: org.ystia.yorc.experimental.consul.linux.ansible.nodes.ConsulServer
      properties:
        install_dnsmasq: true
        mode: server
        tls_enabled: {{.Consul.TLSEnabled}}
        tls_for_checks_enabled: {{.Consul.TLSForChecksEnabled}}
        encrypt_key: "{{.Consul.EncryptKey}}"
        download_url: "{{.Consul.DownloadURL}}"
        install_dir: "/usr/local/bin"
        config_dir: "/etc/consul.d"
        datacenter: dc1
        domain: consul
        web_ui: false
        data_dir: "/var/consul"
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - hostedOnComputeConsulHost:
            type_requirement: host
            node: ConsulCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
      capabilities:
        consul_server:
          properties:
            port: {{.Consul.Port}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
        consul_agent:
          properties:
            port: {{.Consul.Port}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
    AnsibleRuntime:
      type: org.ystia.ansible.linux.ansible.nodes.AnsibleRuntime
      properties:
        component_version: "{{.Ansible.Version}}"
        extra_package_repository_url: "{{.Ansible.PackageRepositoryURL}}"
      requirements:
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: YorcCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
    JDK:
      type: org.alien4cloud.java.jdk.linux.nodes.OracleJDK
      properties:
        java_url: "{{.Jdk.DownloadURL}}"
        java_home: "/opt/java"
        component_version: "{{.Jdk.Version}}"
      requirements:
        - hostedOnAlien4CloudComputeHost:
            type_requirement: host
            node: Alien4CloudCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
    Alien4Cloud:
      type: org.alien4cloud.alien4cloud.webapp.nodes.Alien4Cloud
      properties:
        context_root: "/"
        component_version: "{{getAlien4CloudVersion .Alien4cloud.DownloadURL}}"
        data_dir: "/opt/alien4cloud/data"
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
        extra_env: |{{indent .Alien4cloud.ExtraEnv 10}}
      requirements:
        - javaSoftwareHostedOnJdkOracleJdkJdk:
            type_requirement: java
            node: JDK
            capability: org.alien4cloud.java.pub.capabilities.JDK
            relationship: org.alien4cloud.java.pub.relationships.JavaSoftwareHostedOnJDK
      capabilities:
        consul:
          properties:
            tls_enabled: {{.Consul.TLSEnabled}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
        jolokia:
          properties:
            port: 8778
        http:
          properties:
            port: 9200
            protocol: http
            secure: false
            network_name: PRIVATE
            initiator: source
        rest:
          properties:
            user: {{.Alien4cloud.User}}
            password: {{.Alien4cloud.Password}}
            port: {{.Alien4cloud.Port}}
            protocol: {{.Alien4cloud.Protocol}}
            secure: false
            network_name: PRIVATE
            initiator: source
      artifacts:
        alien_dist:
          file: alien4cloud-dist.tar.gz
          type: tosca.artifacts.File
    YorcPlugin:
      type: org.ystia.yorc.alien4cloud.nodes.YorcProvider
      properties:
        download_url: "{{.YorcPlugin.DownloadURL}}"
        name: Yorc
        discriminator: yorc
        pluginId: "alien4cloud-yorc-provider"
        pluginBean: "yorc-orchestrator-factory"
        yorc_address_ha: "yorc.service.consul"
      requirements:
        - connectsToYorcYorcServerRest:
            type_requirement: yorc
            node: YorcServer
            capability: org.ystia.yorc.pub.capabilities.YorcRestAPI
            relationship: org.ystia.yorc.alien4cloud.relationships.ConnectsToYorc
        - alienConfiguratorHostedOnAlienAlien4CloudConfig:
            type_requirement: host
            node: Alien4Cloud
            capability: org.alien4cloud.alien4cloud.pub.capabilities.AlienConfigContainer
            relationship: org.alien4cloud.alien4cloud.pub.relationships.AlienConfiguratorHostedOnAlien
    YorcOnDemandLocationResources:
      type: org.ystia.yorc.alien4cloud.nodes.YorcOnDemandLocationResourcesConfigurator
      properties:
        type: "{{.Location.Type}}"
        name: "{{.Location.Name}}"
      requirements:
        - locationHostedOnOrchestratorYorcPluginLocation_config:
            type_requirement: orchestrator
            node: YorcPlugin
            capability: org.alien4cloud.alien4cloud.config.pub.capabilities.OrchestratorConfigurator
            relationship: org.alien4cloud.alien4cloud.config.pub.relationships.LocationHostedOnOrchestrator
      artifacts:
        resources_file_artifact:
          file: {{.Location.ResourcesFile}}
    NFSServer:
      type: org.ystia.nfs.linux.ansible.nodes.NFSServer
      requirements:
        - hostedOnComputeNfsHost:
            type_requirement: host
            node: NFSCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
      capabilities:
        nfs_server:
          properties:
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
    TerraformRuntime:
      type: org.ystia.terraform.linux.terraform.nodes.TerraformRuntime
      properties:
        install_dir: "/usr/local/bin"
        plugins_dir: "/var/terraform/plugins"{{formatAsYAML .Terraform 8}}
      requirements:
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: YorcCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
    NFSClient:
      type: org.ystia.nfs.linux.ansible.nodes.NFSClient
      properties:
        fs_type: ext4
        location: "/var/yorc/work"
      requirements:
        - joinNfsServerNfsServerNfs_server:
            type_requirement: partition
            node: NFSServer
            capability: org.ystia.nfs.pub.capabilities.NFSServer
            relationship: org.ystia.nfs.linux.ansible.relationships.JoinNFSServer
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: YorcCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
    ConsulAgentA4C:
      type: org.ystia.yorc.experimental.consul.linux.ansible.nodes.ConsulAgent
      properties:
        install_dnsmasq: true
        mode: agent
        tls_enabled: {{.Consul.TLSEnabled}}
        tls_for_checks_enabled: {{.Consul.TLSForChecksEnabled}}
        encrypt_key: "{{.Consul.EncryptKey}}"
        download_url: "{{.Consul.DownloadURL}}"
        install_dir: "/usr/local/bin"
        config_dir: "/etc/consul.d"
        datacenter: dc1
        domain: consul
        web_ui: false
        data_dir: "/var/consul"
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - joinServerConsulServerConsul_server:
            type_requirement: consul_server
            node: ConsulServer
            capability: org.ystia.yorc.experimental.consul.pub.capabilities.ConsulServer
            relationship: org.ystia.yorc.experimental.consul.linux.ansible.relationships.JoinServer
        - hostedOnAlien4CloudComputeHost:
            type_requirement: host
            node: Alien4CloudCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
      capabilities:
        consul_agent:
          properties:
            port: {{.Consul.Port}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
    ConsulAgentYorcServer:
      type: org.ystia.yorc.experimental.consul.linux.ansible.nodes.ConsulAgent
      properties:
        install_dnsmasq: true
        mode: agent
        tls_enabled: {{.Consul.TLSEnabled}}
        tls_for_checks_enabled: {{.Consul.TLSForChecksEnabled}}
        encrypt_key: "{{.Consul.EncryptKey}}"
        download_url: "{{.Consul.DownloadURL}}"
        install_dir: "/usr/local/bin"
        config_dir: "/etc/consul.d"
        datacenter: dc1
        domain: consul
        web_ui: false
        data_dir: "/var/consul"
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - joinServerConsulServerConsul_server:
            type_requirement: consul_server
            node: ConsulServer
            capability: org.ystia.yorc.experimental.consul.pub.capabilities.ConsulServer
            relationship: org.ystia.yorc.experimental.consul.linux.ansible.relationships.JoinServer
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: YorcCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
      capabilities:
        consul_agent:
          properties:
            port: {{.Consul.Port}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
    {{if not .Insecure}}
    VaultCompute: {{block "Compute" .}} {{end}}
    ConsulAgentVaultServer:
      type: org.ystia.yorc.experimental.consul.linux.ansible.nodes.ConsulAgent
      properties:
        install_dnsmasq: true
        mode: agent
        tls_enabled: {{.Consul.TLSEnabled}}
        tls_for_checks_enabled: {{.Consul.TLSForChecksEnabled}}
        encrypt_key: "{{.Consul.EncryptKey}}"
        download_url: "{{.Consul.DownloadURL}}"
        install_dir: "/usr/local/bin"
        config_dir: "/etc/consul.d"
        datacenter: dc1
        domain: consul
        web_ui: false
        data_dir: "/var/consul"
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - joinServerConsulServerConsul_server:
            type_requirement: consul_server
            node: ConsulServer
            capability: org.ystia.yorc.experimental.consul.pub.capabilities.ConsulServer
            relationship: org.ystia.yorc.experimental.consul.linux.ansible.relationships.JoinServer
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: VaultCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
      capabilities:
        consul_agent:
          properties:
            port: {{.Consul.Port}}
            protocol: tcp
            secure: false
            network_name: PRIVATE
            initiator: source
    VaultServer:
      type: org.alien4cloud.vault.vault_sh.nodes.VaultServer
      properties:
        port: {{.Vault.Port}}
        download_url: "{{.Vault.DownloadURL}}"
        install_dir: "/var/vault"
        tls_disable: false
        auto_unsealed: true
        ldap_enable: false
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - consul:
            node: ConsulAgentVaultServer
            capability: org.ystia.yorc.experimental.consul.pub.capabilities.ConsulServer
            relationship: org.alien4cloud.vault.relationships.ConnectToConsulAgent
        - host:
            node: VaultCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
    {{end}}
    YorcServer:
      type: org.ystia.yorc.linux.ansible.nodes.YorcServer
      properties:
        download_url: "{{.Yorc.DownloadURL}}"
        install_dir: "/usr/local/bin"
        config_dir: "/etc/yorc"
        data_dir: "{{.Yorc.DataDir}}"
        resources_prefix: "{{.Yorc.ResourcesPrefix}}"
        private_key_content: |{{indent .Yorc.PrivateKeyContent 10}}
        ca_pem: |{{indent .Yorc.CAPEMContent 10}}
        ca_key: |{{indent .Yorc.CAKeyContent 10}}
        ca_passphrase: "{{.Yorc.CAPassPhrase}}"
      requirements:
        - hostedOnComputeYorcHost:
            type_requirement: host
            node: YorcCompute
            capability: tosca.capabilities.Compute
            relationship: tosca.relationships.HostedOn
        - joinConsulAgentConsulAgentConsul_agent:
            type_requirement: consul
            node: ConsulAgentYorcServer
            capability: org.alien4cloud.consul.pub.capabilities.ConsulAgent
            relationship: org.ystia.yorc.linux.ansible.relationships.JoinConsulAgent
        {{if not .Insecure}}
        - connectToVaultServer:
            type_requirement: vault
            node: VaultServer
            capability: org.alien4cloud.vault.pub.capabilities.VaultServer
            relationship: org.ystia.yorc.linux.ansible.relationships.ConnectsToVaultServer
        {{end}}
        - dependsOnAnsibleRuntimeAnsibleRuntimeFeature:
            type_requirement: ansible
            node: AnsibleRuntime
            capability: tosca.capabilities.Node
            relationship: org.ystia.ansible.pub.relationships.DependsOnAnsibleRuntime
        - dependsOnTerraformRuntimeTerraformRuntimeFeature:
            type_requirement: terraform
            node: TerraformRuntime
            capability: tosca.capabilities.Node
            relationship: org.ystia.yorc.linux.ansible.relationships.DependsOnTerraformRuntime
        - dependsOnFilessystemNfsClientFeature:
            type_requirement: work_directory
            node: NFSClient
            capability: tosca.capabilities.Node
            relationship: org.ystia.yorc.pub.relationships.DependsOnFilessystem
      capabilities:
        rest:
          properties:
            port: {{.Yorc.Port}}
            protocol: {{.Yorc.Protocol}}
            secure: false
            network_name: PRIVATE
            initiator: source
    {{if not .Address}}
    Alien4CloudCompute: {{block "Compute" .}} {{end}}
    {{else}}
    Alien4CloudCompute: {{block "ComputeWithAddress" .}} {{end}}
    ComputeAddress:  {{block "ComputeAddress" .}} {{end}}
    {{end}}
    YorcCompute: {{block "ComputeScalable" .}} {{end}}
    ConsulCompute: {{block "ComputeScalable" .}} {{end}}
    NFSCompute: {{block "Compute" .}} {{end}}
    Infrastructure: {{block "Infrastructure" .}} {{end}}
    YorcAnsibleConfig:
      type: org.ystia.yorc.ansible.Config
      properties:
        use_openssh: {{.Ansible.UseOpenSSH}}
        inventory: {{formatAsJSON .Ansible.Inventory}}
        host_operations_allowed: {{.Ansible.HostOperationsAllowed}}
      requirements:
        - ansibleYorcServer:
            type_requirement: host
            node: YorcServer
            capability: org.ystia.yorc.pub.capabilities.YorcConfigContainer
            relationship: org.ystia.yorc.linux.ansible.relationships.YorcConfigAnsible
  outputs:
    Alien4Cloud_alien_url:
      value: { get_attribute: [ Alien4Cloud, alien_url ] }
  workflows:
    install:
      steps:
        {{if not .Insecure}}
        VaultCompute_install:
          target: VaultCompute
          activities:
            - delegate: install
          on_success:
            - ConsulAgentVaultServer_initial
        ConsulAgentVaultServer_initial:
          target: ConsulAgentVaultServer
          activities:
            - set_state: initial
          on_success:
            - ConsulAgentVaultServer_creating
        ConsulAgentVaultServer_creating:
          target: ConsulAgentVaultServer
          activities:
            - set_state: creating
          on_success:
            - ConsulAgentVaultServer_create
        ConsulAgentVaultServer_create:
          target: ConsulAgentVaultServer
          activities:
            - call_operation: Standard.create
          on_success:
            - ConsulAgentVaultServer_created
        ConsulAgentVaultServer_created:
          target: ConsulAgentVaultServer
          activities:
            - set_state: created
          on_success:
            - ConsulAgentVaultServer_configuring
        ConsulAgentVaultServer_configuring:
          target: ConsulAgentVaultServer
          activities:
            - set_state: configuring
          on_success:
            - ConsulAgentVaultServer_joinServerConsulServerConsul_server_pre_configure_source
        ConsulAgentVaultServer_joinServerConsulServerConsul_server_pre_configure_source:
          target: ConsulAgentVaultServer
          target_relationship: joinServerConsulServerConsul_server
          operation_host: SOURCE
          activities:
            - call_operation: Configure.pre_configure_source
          on_success:
            - ConsulAgentVaultServer_configure
        ConsulAgentVaultServer_configure:
          target: ConsulAgentVaultServer
          activities:
            - call_operation: Standard.configure
          on_success:
            - ConsulAgentVaultServer_configured
        ConsulAgentVaultServer_configured:
          target: ConsulAgentVaultServer
          activities:
            - set_state: configured
          on_success:
            - ConsulAgentVaultServer_starting
        ConsulAgentVaultServer_starting:
          target: ConsulAgentVaultServer
          activities:
            - set_state: starting
          on_success:
            - ConsulAgentVaultServer_start
        ConsulAgentVaultServer_start:
          target: ConsulAgentVaultServer
          activities:
            - call_operation: Standard.start
          on_success:
            - ConsulAgentVaultServer_started
        ConsulAgentVaultServer_started:
          target: ConsulAgentVaultServer
          activities:
            - set_state: started
          on_success:
            - VaultServer_initial
        VaultServer_initial:
          target: VaultServer
          activities:
            - set_state: initial
          on_success:
            - VaultServer_creating
        VaultServer_creating:
          target: VaultServer
          activities:
            - set_state: creating
          on_success:
            - VaultServer_create
        VaultServer_create:
          target: VaultServer
          activities:
            - call_operation: Standard.create
          on_success:
            - VaultServer_created
        VaultServer_created:
          target: VaultServer
          activities:
            - set_state: created
          on_success:
            - VaultServer_configuring
        VaultServer_configuring:
          target: VaultServer
          activities:
            - set_state: configuring
          on_success:
            - VaultServer_consul_pre_configure_source
        VaultServer_consul_pre_configure_source:
          target: VaultServer
          target_relationship: consul
          operation_host: SOURCE
          activities:
            - call_operation: Configure.pre_configure_source
          on_success:
            - VaultServer_configured
        VaultServer_configured:
          target: VaultServer
          activities:
            - set_state: configured
          on_success:
            - VaultServer_starting
        VaultServer_starting:
          target: VaultServer
          activities:
            - set_state: starting
          on_success:
            - VaultServer_start
        VaultServer_start:
          target: VaultServer
          activities:
            - call_operation: Standard.start
          on_success:
            - VaultServer_started
        VaultServer_started:
          target: VaultServer
          activities:
            - set_state: started
          on_success:
            - YorcServer_initial
        {{end}}
        ConsulAgentYorcServer_start:
          target: ConsulAgentYorcServer
          activities:
            - call_operation: Standard.start
          on_success:
            - ConsulAgentYorcServer_started
        {{if .Address}}
        ComputeAddress_install:
          target: ComputeAddress
          activities:
            - delegate: install
          on_success:
            - Alien4CloudCompute_install
        {{end}}
        ComputeAddress_install:
          target: ComputeAddress
          activities:
            - delegate: install
          on_success:
            - Alien4CloudCompute_install
        JDK_creating:
          target: JDK
          activities:
            - set_state: creating
          on_success:
            - JDK_create
        YorcServer_dependsOnTerraformRuntimeTerraformRuntimeFeature_post_configure_source:
          target: YorcServer
          target_relationship: dependsOnTerraformRuntimeTerraformRuntimeFeature
          operation_host: SOURCE
          activities:
            - call_operation: Configure.post_configure_source
          on_success:
            - Infrastructure_yorcConfigInfrastructureHostedOnYorcYorcServerConfig_post_configure_target
            {{if not .Insecure}}
            - Infrastructure_infraSecretsHostedOnVault_post_configure_source
            {{end}}
            - YorcAnsibleConfig_yorcConfigAnsibleHostedOnYorcYorcServerConfig_post_configure_target
            - YorcServer_configured
        ComputeYorc_install:
          target: YorcCompute
          activities:
            - delegate: install
          on_success:
            - ConsulAgentYorcServer_initial
            - NFSClient_initial
            - YorcServer_initial
        ConsulAgentA4C_configured:
          target: ConsulAgentA4C
          activities:
            - set_state: configured
          on_success:
            - ConsulAgentA4C_starting
        ConsulServer_create:
          target: ConsulServer
          activities:
            - call_operation: Standard.create
          on_success:
            - ConsulServer_created
        AnsibleRuntime_configuring:
          target: AnsibleRuntime
          activities:
            - set_state: configuring
          on_success:
            - AnsibleRuntime_configured
        YorcServer_created:
          target: YorcServer
          activities:
            - set_state: created
