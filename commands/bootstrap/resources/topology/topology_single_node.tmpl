          target: ConsulServer
          activities:
            - set_state: configured
          on_success:
            - ConsulServer_starting
        JDK_configured:
          target: JDK
          activities:
            - set_state: configured
          on_success:
            - JDK_starting
        YorcOnDemandLocationResources_starting:
          target: YorcOnDemandLocationResources
          activities:
