          on_success:
            - YorcOnDemandLocationResources_start
        Alien4Cloud_configuring:
          target: Alien4Cloud
          activities:
            - set_state: configuring
          on_success:
            - Alien4Cloud_configure
        TerraformRuntime_creating:
          target: TerraformRuntime
          activities:
            - set_state: creating
          on_success:
            - TerraformRuntime_create
        YorcServer_creating:
          target: YorcServer
          activities:
            - set_state: creating
          on_success:
            - YorcServer_create
        Alien4Cloud_initial:
